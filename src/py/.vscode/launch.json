{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost",
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        },
        {
            "name": "Python: b1_fill_missing_gas",
            "type": "python",
            "request": "launch",
            "module": "b1_fill_missing_gas",
            "console": "integratedTerminal",
            "args": [ "--dir",
                "/Users/hinashah/UNCFAMLI/Data/StructuredReports/SASUniversityEdition/myfolders/epic",
                "--out_dir",
                "/Users/hinashah/UNCFAMLI/Data/StructuredReports/SASUniversityEdition/myfolders",
                "--db_file",
                "/Users/hinashah/UNCFAMLI/Data/StructuredReports/SASUniversityEdition/myfolders/b1_biom_missinggas.sas7bdat",
                "--debug"
            ]
        },
        {
            "name": "Python: extract_ga_db",
            "type": "python",
            "request": "launch",
            "module": "extract_ga_db",
            "console": "integratedTerminal",
            "args": [ "--dir",
                "/Users/hinashah/famli/Users/hinashah/dataset_C1_tags",
                "--out_dir",
                "/Users/hinashah/famli/Users/hinashah/dataset_C1_cines",
                "--db_file",
                "/Users/hinashah/famli/Groups/Restricted_access_data/Structured_Reports/famli_c1_dicom_sr.sas7bdat"
            ]
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "django": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}